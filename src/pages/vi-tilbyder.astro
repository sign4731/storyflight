---
import { getData } from '../helpers/dataFetching.js';
import BaseLayout from '../layouts/BaseLayout.astro';
import Services from '../layouts/Services.astro'
import ImageAndText from '../layouts/ImageAndText.astro';
import ContactSection from '../layouts/ContactSection.astro';
import Cursor from '../components/Cursor.svelte';

// SEO
const pageData = await getData('pages', 'viTilbyder');

---

<BaseLayout pageContent={pageData}>
    <Cursor client:load />
    <ImageAndText />
    <Services />
    <ContactSection />
</BaseLayout>


